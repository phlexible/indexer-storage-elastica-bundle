{
    "name": "phlexible/indexer-storage-elastica-bundle",
    "description": "phlexible indexer storage elastica bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["elastia", "elasticsearch", "indexer"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\IndexerStorageElasticaBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "phlexible/indexer-bundle": "^1.2",
        "phlexible/elastica-bundle": "~1.2",
        "symfony/console": "^2.8",
        "symfony/dependency-injection": "^2.8",
        "symfony/http-kernel": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "phlexible/problem-bundle": "^1.3.6"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
