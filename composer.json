{
    "name": "phlexible/indexer-storage-elastica-bundle",
    "description": "phlexible indexer storage elastica bundle",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\IndexerStorageElasticaBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.3.0",
        "phlexible/indexer-bundle": "~1.0",
        "phlexible/elastica-bundle": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "sami/sami": "~2.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.brainbits.net/phlexible-bundles/",
            "options": {
                "ssl": {
                    "allow_self_signed": "true"
                }
            }
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
